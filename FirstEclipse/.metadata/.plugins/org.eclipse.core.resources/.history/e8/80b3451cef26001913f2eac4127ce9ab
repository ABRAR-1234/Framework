package com.Veh;

public class Car extends Veh{
	
	private int doors;
	private int wheels;
	private boolean isManual;
	private int gears;
	
	private int currentgear;
	
	public Car(String name, int size,int doors,int gears,int wheels,boolean isManual) {
		super(name, size);
		  this.doors = doors;
		  this.gears = gears;
		  this.isManual = isManual;
		  this.wheels = wheels;
		  
	}
	
	public void changegear(int gear)
	{
		this.currentgear = gear;
		System.out.println("The current gear is:" + this.currentgear);
	}
	
	public void changevelocity(int speed,int direction)
	{
		System.out.println("the changed velocity:" + speed + "direction:" + direction);
		move(speed,direction);	
	}
	
	
	
	

	
	
	

}
