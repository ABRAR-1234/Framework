package com.testPages;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class Inbox {

 @FindBy(xpath = "//*[@id=\":kg\"]/div/div")
 WebElement compose;
 
 public void Compose()
 {
	 compose.click();
 }
	
 @FindBy(xpath = "//*[@id=\":qi\"]")
 WebElement To;
 
 public void to(String to) {
	 
	 To.sendKeys(to);
 }
	
 @FindBy(xpath = "//*[@id=\":q0\"]")
 WebElement sub;
 
 public void Subject(String Subj) {
	 
	 To.sendKeys(Subj);
 }
	
 @FindBy(xpath = "//*[@id=\":ri\"]")
 WebElement attch;
 
 
 public void attach() {
	 attch.click();
 }
 
 
 
	
	
	
	
	
	
	
}
